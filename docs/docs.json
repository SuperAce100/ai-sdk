{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#00B7EB",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Overview",
        "groups": [
          {
            "group": "Introduction",
            "pages": ["sdk/introduction", "sdk/concepts"]
          },
          {
            "group": "Guides",
            "pages": [
              "examples/basic-text",
              "examples/streaming",
              "examples/structured-output",
              "examples/tool-agent",
              "examples/embeddings-search"
            ]
          }
        ]
      },
      {
        "tab": "Reference",
        "groups": [
          {
            "group": "Functions",
            "pages": [
              "sdk/text-generation",
              "sdk/object-generation",
              "sdk/embeddings",
              "sdk/tool-calling"
            ]
          },
          {
            "group": "Providers",
            "pages": ["sdk/providers"]
          },
          {
            "group": "Types",
            "pages": ["sdk/types"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/python-ai-sdk/sdk",
          "icon": "github"
        },
        {
          "anchor": "PyPI",
          "href": "https://pypi.org/project/ai-sdk-python/",
          "icon": "python"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "perplexity", "mcp", "cursor", "vscode"]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
